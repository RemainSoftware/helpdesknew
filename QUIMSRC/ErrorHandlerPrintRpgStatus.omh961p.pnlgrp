.*        Program code
.*        ��������������������������������������������������
:VAR      NAME=FMPGMC
          CLASS=OBJC.

:VAR      NAME=FMLIBC
          CLASS=OBJC.

.*        ��������������������������������������������������
.*        RPG Exception code
.*        ��������������������������������������������������
:VAR      NAME=FMREXC
          CLASS=GC04.

.*        ��������������������������������������������������
.*        Reserved
.*        ��������������������������������������������������
:VAR      NAME=FMR1
          CLASS=GC30.

:VAR      NAME=FMR2
          CLASS=GC80.

:VAR      NAME=FMR3
          CLASS=GC24.

:VAR      NAME=FMR4
          CLASS=GZ20.

:VAR      NAME=FMR5
          CLASS=GC35.

.*        ��������������������������������������������������
.*        Status codes
.*        ��������������������������������������������������
:VAR      NAME=FMSTAC
          CLASS=STAC.

:VAR      NAME=FMPSTD
          CLASS=STAD.

:VAR      NAME=FMPSTC
          CLASS=STAC.

:VAR      NAME=FMFSTC
          CLASS=STAC.

:VAR      NAME=FMFSTD
          CLASS=STAD.

